<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="TreeViewItem_BackgroundNormal" Color="Transparent"/>
    <SolidColorBrush x:Key="TreeViewItem_BackgroundSelected" Color="Green"/>
    <SolidColorBrush x:Key="TreeViewItem_BackgroundLostFocus" Color="Gray"/>

    <Color x:Key="TreeViewItem_BackgroundNormalColor">Transparent</Color>
    <Color x:Key="TreeViewItem_BackgroundSelectedColor">Green</Color>
    <Color x:Key="TreeViewItem_BackgroundLostFocusColor">Gray</Color>

</ResourceDictionary>