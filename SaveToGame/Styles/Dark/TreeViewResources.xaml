<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <SolidColorBrush x:Key="TreeView_BackgroundNormal" Color="{DynamicResource ContainerBackgroundNormal}"/>
    <SolidColorBrush x:Key="TreeView_BorderNormal" Color="{DynamicResource BorderNormalColor}"/>

    <SolidColorBrush x:Key="TreeView_BackgroundDisabled" Color="{DynamicResource ContainerBackgroundNormal}" Opacity="0.7"/>
    <SolidColorBrush x:Key="TreeView_BorderDisabled" Color="{DynamicResource BorderNormalColor}" Opacity="0.7"/>
    
</ResourceDictionary>