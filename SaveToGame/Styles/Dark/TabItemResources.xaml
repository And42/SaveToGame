<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="TabItem_BackgroundNormal" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#333337" Offset="0.2"/>
        <GradientStop Color="#414148" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="TabItem_BackgroundOver" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#333337" Offset="0.2"/>
        <GradientStop Color="#414148" Offset="1"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="TabItem_BackgroundPressed" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Color="#414148" Offset="0.2"/>
        <GradientStop Color="#333337" Offset="1"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="TabItem_BorderNormal"   Color="{DynamicResource BorderNormalColor}"/>
    <SolidColorBrush x:Key="TabItem_BorderOver"     Color="{DynamicResource HighlitedColor}"/>
    <SolidColorBrush x:Key="TabItem_BorderPressed"  Color="{DynamicResource HighlitedColor}"/>

</ResourceDictionary>