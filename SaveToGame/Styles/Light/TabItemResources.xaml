<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <LinearGradientBrush x:Key="TabItem_BackgroundNormal" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#E0E0E0"/>
        <GradientStop Offset="1" Color="#F0F0F0"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="TabItem_BackgroundOver" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#DCECFC"/>
        <GradientStop Offset="1" Color="#ECF4FC"/>
    </LinearGradientBrush>
    <LinearGradientBrush x:Key="TabItem_BackgroundPressed" StartPoint="0,0" EndPoint="0,1">
        <GradientStop Offset="0" Color="#F0F0F0"/>
        <GradientStop Offset="1" Color="#D5D5D5"/>
    </LinearGradientBrush>
    
    <SolidColorBrush x:Key="TabItem_BorderNormal" Color="{DynamicResource BorderNormalColor}"/>
    <SolidColorBrush x:Key="TabItem_BorderOver" Color="#569DE5"/>
    <SolidColorBrush x:Key="TabItem_BorderPressed" Color="{DynamicResource HighlitedColor}"/>

</ResourceDictionary>